<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2D3447;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignupController">
   <children>
      <JFXTextField fx:id="username" layoutX="209.0" layoutY="108.0" prefHeight="26.0" prefWidth="242.0" promptText="Username" />
      <JFXPasswordField fx:id="password" layoutX="209.0" layoutY="159.0" prefHeight="26.0" prefWidth="242.0" promptText="Password" />
      <JFXRadioButton fx:id="male" layoutX="209.0" layoutY="212.0" text="Male" textFill="WHITE">
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </JFXRadioButton>
      <JFXRadioButton fx:id="other" layoutX="384.0" layoutY="212.0" text="Other" textFill="WHITE" toggleGroup="$gender" />
      <ImageView fitHeight="26.0" fitWidth="20.0" layoutX="149.0" layoutY="111.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8_User_Male_26px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="36.0" fitWidth="20.0" layoutX="147.0" layoutY="162.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8_Lock_2_64px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="36.0" fitWidth="20.0" layoutX="147.0" layoutY="211.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8_Genderqueer_26px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="36.0" fitWidth="20.0" layoutX="147.0" layoutY="259.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8_Marker_26px.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="location" layoutX="211.0" layoutY="258.0" prefHeight="26.0" prefWidth="242.0" promptText="Location" />
      <JFXButton fx:id="signup" layoutX="160.0" layoutY="306.0" onAction="#signupAction" prefHeight="38.0" prefWidth="294.0" style="-fx-background-color: #e2e2e2;" text="Sign Up" textFill="#443f3f">
         <font>
            <Font size="16.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="login" layoutX="463.0" layoutY="25.0" onAction="#loginAction" prefHeight="38.0" prefWidth="79.0" text="Login" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </JFXButton>
      <ImageView fitHeight="56.0" fitWidth="89.0" layoutX="272.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8_Car_50px.png" />
         </image>
      </ImageView>
      <Label layoutX="249.0" layoutY="72.0" text="Car" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label layoutX="281.0" layoutY="69.0" text="Review" textFill="#e5d5d5">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <ImageView fx:id="progress" fitHeight="18.0" fitWidth="222.0" layoutX="194.0" layoutY="355.0" pickOnBounds="true">
         <image>
            <Image url="@../../Resources/294-1.gif" />
         </image>
      </ImageView>
      <JFXRadioButton fx:id="female" layoutX="294.0" layoutY="212.0" text="Female" textFill="WHITE" toggleGroup="$gender" />
   </children>
</AnchorPane>
